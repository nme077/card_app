<div class="col-12 text-center dropzone" style="height: 75%; width: 100%;">
   <!-- must be valid image url-->
   <img class="mx-auto card-image" style="max-width: 85%; margin-top: 5%;" name="card[image]" src="<%= card.images[0].image !== undefined ? card.images[0].image : '' %>">
   
</div>
<div class="col-12 text-center">
   <input type="text" class="text-center message" style="width: 100%; margin-top: 8%;" name="card[message]" placeholder="Type your message here" value="<%= card.messages[0].message %>">
</div>